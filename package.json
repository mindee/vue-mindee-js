{
  "name": "vue-mindee-js",
  "version": "1.3.1",
  "description": "Computer Vision SDK for Vue",
  "author": "@mindee",
  "license": "MIT",
  "repository": "publicMindee/vue-mindee-js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build & yarn tsc",
    "serve": "vite preview",
    "cy:prepare": "yarn build",
    "cy:open": "yarn --cwd ./cypress cypress open-ct",
    "cy:run": "yarn --cwd ./cypress cypress run-ct",
    "docs:start": "yarn --cwd ./docs start",
    "docs:install": "yarn --cwd ./docs install",
    "docs:prepare": "yarn build && yarn docs:install",
    "docs:update": "yarn --cwd ./docs add ../",
    "docs:build": "yarn --cwd ./docs build",
    "docs:deploy": "yarn --cwd ./docs deploy"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "konva": "^8.3.10",
    "pdfjs-dist": "^2.9.359",
    "ts-key-enum": "^2.0.7",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.0.8",
    "@cypress/vue": "^3.0.3",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/composition-api": "^1.0.0-rc.1",
    "cypress": "^8.3.1",
    "cypress-image-snapshot": "^4.0.1",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.1",
    "vite-babel-plugin": "^0.0.2",
    "vue": "^3.2.6",
    "vue-tsc": "^0.3.0"
  },
  "keywords": [
    "vue",
    "js",
    "javascript",
    "mindee",
    "SDK",
    "canvas",
    "document",
    "image",
    "segmentation",
    "processing",
    "computer",
    "vision"
  ]
}
